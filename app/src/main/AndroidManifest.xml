<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.yogi.tp"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".TP"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- Notice this tag which provides the database name -->
        <meta-data
            android:name="AA_DB_NAME"
            android:value="teeproovidebaas.db"/>

        <!-- Notice this tag which provides the database version -->
        <meta-data
            android:name="AA_DB_VERSION"
            android:value="1"/>

        <activity android:name=".UusObjektActivity">
        </activity>
        <activity android:name=".EsilehtActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ObjektideNimekiriActivity">
        </activity>
        <activity android:name=".YksObjektNimekirjasActivity">
        </activity>
        <activity android:name=".YksProovNimekirjasActivity">
        </activity>
        <activity android:name=".ProovideNimekiriActivity">
        </activity>
    </application>

</manifest>
